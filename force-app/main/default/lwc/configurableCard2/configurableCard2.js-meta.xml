<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>48.0</apiVersion>
    <isExposed>true</isExposed>
    <targets>
        <target>lightningCommunity__Page</target>
        <target>lightningCommunity__Default</target>
    </targets>
    <!-- Configuring the design attributes -->
    <targetConfigs>
        <targetConfig targets="lightningCommunity__Default">
            <property name="cardHeadline" type="String" default="Enter a Title Here" label="Card Title" />
            <property name="picFlip" type="Boolean" default="false" label="Do you want to flip picture ?"  />
            <property name="imageURL" type="String"  />
            <property name="webURL" type="String"  />
            <property name="descriptionURL" type="String" default="URL Label" />
        </targetConfig>
    </targetConfigs>

</LightningComponentBundle>